                    <CommandText>SELECT        TblBean.Beanid, CdBeanType.BeanType, cdStage.StageName, cdExam.ExamName, cdYear.YearName, TblBean.stname, cdSchool.school, TblBean.gloosno, cdShopa.ShopaName, CdFaslDrasy.FaslDrasy, 
